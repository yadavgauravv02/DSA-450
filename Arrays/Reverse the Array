//Shinchan Loves Coding :))
#include<bits/stdc++.h>
 
using namespace std;

inline void runn() {
    #ifndef ONLINE_JUDGE                 
    freopen("input.txt","r",stdin);   
    freopen("output.txt","w",stdout);   
    freopen("error.txt","w",stderr);   
    #endif                               
}  

// Auxillary Space => null
// Time Complexity => O(n)
// Space Complexity => O(n)

int main() { 
	runn();                         
    int n;
    cin>>n;
    int ar[n];
    for(int i=0 ; i<n ; i++) {
    	cin>>ar[i] ;
    }
    int mid=n/2;
    for(int i=0 ; i<mid ; i++) {
    	swap(ar[i],ar[n-i-1]);
    }
    for(int i=0 ; i<n;  i++) {
    	cout<<ar[i]<<" ";
    }
}
